{
  "name": "fare-formatting-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "jest --silent --noStackTrace --randomize",
    "test:watch": "jest --verbose --detectOpenHandles --no-cache --watch",
    "lint": "eslint 'src/**/*.ts' --quiet --fix",
    "validate": "sls package",
    "install:dependencies": "npm i",
    "debug": "node --inspect node_modules/serverless/bin/serverless offline -s dev",
    "code:offline": "sls offline start",
    "upload-data": "npx ts-node ./src/cli/upload-data.ts",
    "deploy:dev": "STAGE=dev sls deploy --stage dev && npm run upload-data",
    "deploy:prod": "STAGE=prod sls deploy --stage prod && npm run upload-data"
  },
  "engines": {
    "node": ">=14.21.3"
  },
  "dependencies": {
    "@fastify/aws-lambda": "^3.3.0",
    "@fastify/swagger": "^8.6.0",
    "@fastify/swagger-ui": "^1.9.2",
    "fastify": "^4.19.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/aws-lambda-fastify": "^1.4.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "aws-sdk": "^2.1409.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "Ivan",
  "license": "MIT"
}
